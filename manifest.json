{
  "manifest_version": 2,
  "name": "TagHeart",
  "version": "0.1",
  "web_accessible_resources": [
    "*.png",
    "*.mp3",
    "*.woff2"
  ],
  "background": {
      "scripts": ["js/teamSettings.js"],
      "persistent": false
    },
  "content_scripts": [
    {
      "matches": [ "http://tagpro-maptest.koalabeast.com/*", "http://tagpro-origin.koalabeast.com/*", "http://tagpro-chord.koalabeast.com/*", "http://tagpro-pi.koalabeast.com/*", "http://tagpro-centra.koalabeast.com/*", "http://tagpro-diameter.koalabeast.com/*", "http://tagpro-radius.koalabeast.com/*", "http://tagpro-sphere.koalabeast.com/*" ],
      "css": ["css/fonts.css", "css/GameStyles.css"],
      "js": ["js/jquery-2.1.1.min.js", "js/utility.js", "js/initGameView.js","js/GameView.js", "js/GameModel.js", "js/mainMVC.js"]
    }
  ],
  "permissions": ["storage", "tabs"],
  "icons": {
    "128": "images/icon.png"
  },
  "browser_action": {
    "default_icon": {
      "19": "images/icon19.png"
    },
    "default_title": "TagHeart",
    "default_popup": "popup/options.html"
  }
}